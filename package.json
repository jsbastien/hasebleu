{
  "name": "hasebleu",
  "version": "0.9.1",
  "description": "Pageboard hasebleu site",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsbastien/hasebleu.git"
  },
  "author": "Jérémy Lal <kapouer@melix.org>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/jsbastien/hasebleu/issues"
  },
  "homepage": "https://github.com/jsbastien/hasebleu#readme",
  "dependencies": {
    "@pageboard/calendar": "^0.9.2",
    "@pageboard/email": "^0.9.3",
    "@pageboard/feed": "^0.9.6",
    "@pageboard/gallery": "^0.9.0",
    "@pageboard/google": "^0.9.2",
    "@pageboard/html": "^0.9.1",
    "@pageboard/print": "^0.9.0",
    "@pageboard/site": "^0.9.6",
    "@pageboard/write": "^0.9.1"
  },
  "pageboard": {
    "server": "0.8",
    "directories": [
      "elements",
      "ui"
    ],
    "elements": [
      "elements/"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "block-closing-brace-empty-line-before": null,
      "at-rule-empty-line-before": null,
      "selector-descendant-combinator-no-non-space": [
        true,
        {
          "severity": "warning"
        }
      ],
      "length-zero-no-unit": null,
      "color-hex-length": null,
      "color-hex-case": null,
      "selector-list-comma-newline-after": null,
      "selector-pseudo-element-colon-notation": null,
      "max-empty-lines": null,
      "comment-whitespace-inside": null,
      "no-descending-specificity": null,
      "indentation": [
        "tab",
        {
          "severity": "warning"
        }
      ],
      "rule-empty-line-before": null,
      "comment-empty-line-before": null,
      "declaration-colon-space-after": null,
      "selector-type-no-unknown": [
        true,
        {
          "ignore": [
            "custom-elements"
          ]
        }
      ]
    }
  }
}
